# === CELL REPAIR METADATA ===
# Jalankan ini SEBELUM men-download notebook

import json
import os

# Load notebook
notebook_path = '/content/task1_emotion_classification.ipynb'

# Jika belum disave, save dulu
if not os.path.exists(notebook_path):
    print("Saving notebook first...")
    # Save current notebook
    from google.colab import _message
    _message.blocking_request('save_notebook', {})

# Baca file
with open(notebook_path, 'r') as f:
    nb = json.load(f)

# Clean metadata
if 'metadata' in nb:
    # Hapus widgets yang bermasalah
    if 'widgets' in nb['metadata']:
        del nb['metadata']['widgets']
    
    # Pastikan kernel info ada
    if 'kernelspec' not in nb['metadata']:
        nb['metadata']['kernelspec'] = {
            "display_name": "Python 3",
            "language": "python",
            "name": "python3"
        }
    
    # Language info
    if 'language_info' not in nb['metadata']:
        nb['metadata']['language_info'] = {
            "name": "python",
            "version": "3.10.12"
        }

# Simpan kembali
with open(notebook_path, 'w') as f:
    json.dump(nb, f, indent=2)

print("Notebook metadata cleaned successfully!")
print("Now download using: File → Download → Download .ipynb")
